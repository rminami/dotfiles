	"Print line": {
		"prefix": "sout",
		"body": [
			"System.out.println($1);",
		],
		"description": "Print line to stdout"
	},
	"Print format": {
		"prefix": "souf",
		"body": [
			"System.out.printf($1);",
		],
		"description": "Print formatted output to stdout"
	},
	"Main method": {
		"prefix": "psvm",
		"body": [
			"public static void main(String[] args) {",
			"\t$1",
			"}"
		],
		"description": "Main method"
	}
